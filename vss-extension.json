{
  "manifestVersion": 1,
  "id": "vixkapim",
  "name": "VivvixKantar API Management Suite",
  "version": "3.6.8",
  "publisher": "VivvixKantar",
  "tags": [
    "API",
    "Security"
  ],
  "content": {
    "details": {
      "path": "overview.md"
    }
  },
  "public": true,
  "links": {
    "getstarted": {
      "uri": "https://github.com/AdIntelKantarNumerator/azure-apim-extension"
    },
    "support": {
      "uri": "https://github.com/AdIntelKantarNumerator/azure-apim-extension/issues"
    }
  },
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "Broad support of Azure API Management",
  "categories": [
    "Azure Pipelines"
  ],
  "icons": {
    "default": "images/extension-icon.png"
  },
  "files": [
    {
      "path": "vixkapim"
    },
    {
      "path": "vixkapimoperationpolicies"
    },
    {
      "path": "images",
      "addressable": true
    }
  ],
  "contributions": [
    {
      "id": "vixkapim",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "vixkapim"
      }
    },
    {
      "id": "vixkapimoperationpolicies",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "vixkapimoperationpolicies"
      }
    }
  ]
}