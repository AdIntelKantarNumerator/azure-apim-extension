{
  "manifestVersion": 1,
  "id": "apim",
  "name": "API Management Suite",
  "version": "2.0.2",
  "publisher": "stephane-eyskens",
  "tags": [
    "API",
    "Security"
  ],
  "content": {
    "details": {
      "path": "overview.md"
    }
  },
  "public": true,
  "links": {
    "getstarted": {
      "uri": "https://github.com/stephaneey/azure-apim-extension"
    },
    "support": {
      "uri": "https://github.com/stephaneey/azure-apim-extension/issues"
    }
  },
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "Makes sure API endpoints are secured",
  "categories": [
    "Build and release"
  ],
  "icons": {
    "default": "images/extension-icon.png"
  },
  "files": [
    {
      "path": "apim"
    },
    {
      "path": "apimversioned"
    },
    {
      "path": "apimcreateorupdateproduct"
    },
    {
      "path": "apimfunction"
    },
    {
      "path": "apimfunctionversioned"
    },
    {
      "path": "apisecuritychecker"
    },
    {
      "path": "images",
      "addressable": true
    }
  ],

  "contributions": [
    {
      "id": "apim",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "apim"
      }
    },
    {
      "id": "apimversioned",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "apimversioned"
      }
    },
    {
      "id": "apisecuritychecker",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "apisecuritychecker"
      }
    },
    {
      "id": "apimcreateorupdateproduct",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "apimcreateorupdateproduct"
      }
    },
    {
      "id": "apimfunction",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "apimfunction"
      }
    },
    {
      "id": "apimfunctionversioned",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "apimfunctionversioned"
      }
    }
  ]
}